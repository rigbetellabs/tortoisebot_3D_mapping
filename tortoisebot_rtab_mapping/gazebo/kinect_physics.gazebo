<?xml version="1.0"?>
<robot name="tortoisebot" xmlns:xacro="http://www.ros.org/wiki/xacro">

	<!--  ##########  STANDARD FRICTION  ##########  -->

	<gazebo reference="kinect_base">
		<mu1>${standard_friction}</mu1>
		<mu2>${standard_friction}</mu2>
		<selfCollide>true</selfCollide>
	</gazebo>

	<gazebo reference="kinect_sensor">
		<mu1>${standard_friction}</mu1>
		<mu2>${standard_friction}</mu2>
		<selfCollide>true</selfCollide>
	</gazebo>


</robot>